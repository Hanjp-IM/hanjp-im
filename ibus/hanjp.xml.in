<?xml version=\"1.0\" encoding=\"utf-8\"?>
<!-- filename: enchant.xml -->
<component>
	<name>org.ubuntu-kr.IBus.Hanjp</name>
	<description>Hanjp Component</description>
	<exec>/usr/libexec/ibus-engine-hanjp --ibus</exec>
	<version>${CMAKE_PROJECT_VERSION}</version>
	<author>Ubuntu Korea Community</author>
	<license>GPL</license>
	<homepage>https://github.com/Hanjp-IM/ibus-hanjp</homepage>
	<textdomain>${HANJP_TEXT_DOMAIN}</textdomain>

	<engines>
		<engine>
			<name>hanjp</name>
			<language>ja</language>
			<license>GPL v3.0</license>
			<author>Ubuntu Korea community</author>
			<icon>@IBUS_DIR_PREFIX@/ibus-hanjp.svg</icon>
			<layout>us</layout>
			<longname>Hanjp</longname>
			<description>Hanjp Ibus Engine</description>
			<rank>0</rank>
		</engine>
	</engines>

</component>
